@model Department
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit @Model.Name Department</h1>

@* show Error Message*@
<div asp-validation-summary="All"></div>

<form asp-action="Edit" method="post">


    <partial name="PartialViews/CreateEditPartialView"/>



    <input  type="submit" value="Save" class="btn btn-primary"/> |
    <a asp-action="Index" asp-controller="Department" class="btn btn-light">Back To List</a>
</form>



@section ClientSideValidation {
    <partial name="_ValidationScriptsPartial"/>
}


