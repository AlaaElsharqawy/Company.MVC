@model RoleViewModel
@{
    ViewData["Title"] = "Create";
}

<h1>Add New Role</h1>
<br />

<form asp-action="Create" >




    


    <div class="form-group">
        <label class="col-form-label" asp-for="RoleName"></label>
        <input class="form-control" asp-for="RoleName" />

    </div>



    <input type="submit" value="Submit" class="btn btn-primary" /> |
    <a asp-action="Index" asp-controller="Role" class="btn btn-light">Back To List</a>


</form>


@section ClientSideValidation {
    <partial name="_ValidationScriptsPartial" />
}

